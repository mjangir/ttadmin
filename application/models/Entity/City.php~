<?php



use Doctrine\ORM\Mapping as ORM;

/**
 * City
 *
 * @ORM\Table(name="city")
 * @ORM\Entity
 */
class City
{
    /**
     * @var integer
     *
     * @ORM\Column(name="id", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $id;

    /**
     * @var integer
     *
     * @ORM\Column(name="state_id", type="integer", nullable=false)
     */
    private $stateId;

    /**
     * @var string
     *
     * @ORM\Column(name="city_name", type="string", length=100, nullable=false)
     */
    private $cityName;


}
